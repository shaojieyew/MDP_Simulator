<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.collections.FXCollections?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="rootPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" prefHeight="656.0" prefWidth="656.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainController">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <center>
      <BorderPane fx:id="mapPane" prefHeight="656.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <right>
            <VBox prefHeight="200.0" prefWidth="190.0" spacing="5.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="loadMapFileBtn" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onMouseClicked="#onclickLoadMapFileBtn" prefHeight="25.0" prefWidth="132.0" text="Load Map Descriptor" />
                  <Button fx:id="generateMapStatus" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onMouseClicked="#onclickGenerateMapStatus" prefHeight="25.0" prefWidth="132.0" text="Export Map Descriptor" />
                  <Button fx:id="editObstacle" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onMouseClicked="#onclickEditObstacle" prefHeight="25.0" prefWidth="132.0" text="Edit/View Obstacle" />
                  <Button fx:id="resetBtn" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onMouseClicked="#onclickResetSimulator" prefHeight="25.0" prefWidth="189.0" text="Reset" />
                  <Button fx:id="hackAllTile" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onclickDiscoverAll" prefHeight="25.0" prefWidth="162.0" text="Uncover All Square" />
                  <Button fx:id="hideAllTile" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onClickHideAll" prefHeight="25.0" prefWidth="162.0" text="Hide All Square" />
                  <Label text="Robot Coord:" />
                  <HBox>
                     <children>
                        <TextField fx:id="robotXpos" onKeyReleased="#robotPosXChanged" text="1" />
                        <TextField fx:id="robotYpos" onKeyReleased="#robotPosYChanged" text="1" />
                     </children>
                  </HBox>
                <Label text="Way Point: " />
                  <HBox>
                     <children>
                        <TextField fx:id="waypointXpos" onKeyReleased="#waypointPosChanged" text="1" />
                        <TextField fx:id="waypointYpos" onKeyReleased="#waypointPosChanged" text="1" />
                     </children>
                  </HBox>
                  <HBox prefHeight="14.0" prefWidth="190.0" spacing="20.0">
                     <children>
                        <Label text="Forward (cm):" />
                        <Label text="Rotate(degree):" />
                     </children>
                  </HBox>
                  <HBox>
                     <children>
                        <TextField fx:id="forwardTextField" maxWidth="1.7976931348623157E308" prefHeight="25.0" prefWidth="39.0" text="10" />
                        <Button mnemonicParsing="false" onAction="#onClickMoveForward" prefHeight="25.0" prefWidth="56.0" text="Move" />
                        <TextField fx:id="rotateTextField" maxWidth="1.7976931348623157E308" prefHeight="25.0" prefWidth="34.0" text="90" />
                        <Button mnemonicParsing="false" onAction="#onclickRotate" text="Rotate" />
                     </children>
                  </HBox>
                  <HBox />
                  <HBox prefHeight="23.0" prefWidth="190.0">
                     <children>
                        <Label prefHeight="17.0" prefWidth="125.0" text="Speed: milsec/Sq" />
                        <TextField fx:id="travelSpeedTextField" onKeyReleased="#onTravelSpeedChange" prefHeight="25.0" prefWidth="92.0" text="1000" />
                     </children>
                  </HBox>
                  <HBox prefHeight="39.0" prefWidth="190.0">
                     <children>
                        <Label prefHeight="17.0" prefWidth="113.0" text="Speed: milsec/Rotate" />
                        <TextField fx:id="rotateSpeedTextField" onKeyReleased="#onRotatSpeedChange" prefHeight="25.0" prefWidth="80.0" text="1000" />
                     </children>
                  </HBox>
                  <HBox prefHeight="42.0" prefWidth="190.0">
                     <children>
                        <Label prefHeight="17.0" prefWidth="94.0" text="Sensor Type" />
                        <ComboBox fx:id="sensorCombo" maxWidth="1.7976931348623157E308" onAction="#onSensorSelected" prefWidth="90.0" promptText="choose">
					            <items>
					              <FXCollections fx:factory="observableArrayList">
					                <String fx:value="type1" />
					                <String fx:value="type2" />
					                <String fx:value="type3" />
					              </FXCollections>
					            </items>
					      </ComboBox>
                     </children>
                  </HBox>
                 
                  <Button fx:id="exploreBtn1" mnemonicParsing="false" onMouseClicked="#onclickAlgo1" prefHeight="25.0" prefWidth="216.0" text="Start Exploration">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="stopBtn" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onMouseClicked="#onclickStopExploration" prefHeight="25.0" prefWidth="140.0" text="Terminate Exploration" />
                  <Button fx:id="startEndBtn" mnemonicParsing="false" prefHeight="25.0" prefWidth="235.0" text="Start Fastest Path" />
                  <Label fx:id="Timer" />
               </children>
            </VBox>
         </right>
         <bottom>
            <TextArea fx:id="textArea" prefHeight="52.0" prefWidth="549.0" BorderPane.alignment="CENTER" />
         </bottom></BorderPane>
   </center>
</BorderPane>
